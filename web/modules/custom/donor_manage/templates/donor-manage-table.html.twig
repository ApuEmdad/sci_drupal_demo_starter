<h1>Donor's Donation Information</h1>

<div class="donor-data-table-wrapper">
  <table class="donor-data-table">
    <thead>
      <tr>
        {% for heading in header %}
          <th>{{ heading }}</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
      {% if rows %}
        {% for row in rows %}
          <tr>
            {% for column in row %}
              <td>{{ column }}</td>
            {% endfor %}
          </tr>
        {% endfor %}
      {% else %}
        <tr>
          <td colspan="{{ header|length }}">{{ empty }}</td>
        </tr>
      {% endif %}
    </tbody>
  </table>
</div>

